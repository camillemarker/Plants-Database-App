<%- include('../partials/header') %>
<h1>My Garden</h1>
<h2>Plants</h2>
<table>
  <thead>
    <tr>
      <th>Name</th>
    </tr>
  </thead>
  <tbody>
    <% plants.forEach(function(p) { %>
      <tr>
        <td><%= p.name %></td>
        <td><a href="/plants/<%= p._id %>">Details</a></td>
      </tr>
      <% }) %>
  </tbody>
</table>

<%- include('../partials/footer') %>